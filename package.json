{
  "name": "@erdiko/ngx-user-admin",
  "version": "1.1.25",
  "description": "Erdiko Angular User Admin",
  "scripts": {
    "transpile": "ngc --p tsconfig-build.json",
    "package": "rollup -c --silent",
    "minify": "uglifyjs dist/bundles/ngx-user-admin.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-user-admin.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify",
    "clean": "rimraf src/*.ngfactory.* src/*.ngsummary.* src/**/*.ngsummary.* && rimraf src/**/*ngfactory.*",
    "docs": "npm run clean && compodoc -p src/tsconfig.json",
    "start": "npm run tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "test": "npm run clean && npm run tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "tsc": "tsc"
  },
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Erdiko/ngx-user-admin.git"
  },
  "author": "Andy Armstrong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Erdiko/ngx-user-admin/issues"
  },
  "homepage": "https://github.com/Erdiko/ngx-user-admin#readme",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "^2.4.10",
    "@angular/compiler-cli": "^2.4.10",
    "@angular/core": "^2.4.10",
    "@angular/forms": "~2.4.0",
    "@angular/http": "^2.4.10",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/platform-server": "^2.4.10",
    "@angular/router": "~3.4.0",
    "@types/jasmine": "^2.5.46",
    "@types/node": "^6.0.65",
    "@types/selenium-webdriver": "^2.53.42",
    "angular-in-memory-web-api": "~0.2.4",
    "canonical-path": "0.0.2",
    "concurrently": "^3.5.0",
    "core-js": "^2.4.1",
    "html-minifier": "^3.5.2",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^1.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "ngx-bootstrap": "1.6.6",
    "protractor": "~4.0.14",
    "reflect-metadata": "^0.1.8",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.6",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.4.2",
    "rxjs": "^5.0.1",
    "systemjs": "0.19.40",
    "tsc": "^1.20150623.0",
    "tslib": "^1.7.1",
    "tslint": "^4.0.2",
    "typescript": "^2.4.1",
    "uglify-js": "^2.8.22",
    "webdriver-manager": "10.2.10",
    "zone.js": "^0.7.4"
  }
}
